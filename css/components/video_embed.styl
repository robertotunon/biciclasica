/*_______ Embeded Videos _______*/
.video_container
	position relative
	display block
	height 0
	overflow hidden
	margin 0 0 2rem 0
	padding-bottom 56.25% /* 16:9 */
	@media (max-width: 480px)
		margin 0
	iframe, object, embed
		position absolute
		top 0
		left 0
		max-width 100%
		width 100%
		height 100%